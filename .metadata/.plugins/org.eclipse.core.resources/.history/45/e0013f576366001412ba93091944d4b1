package slinkedlist;

class Node
{
	int data;
	Node link;
   public Node()
   {
	   this.data=0;
	   this.link=null;
   }
   public Node(int data,Node link)
   {
	   this.data=data;
	   this.link=link;
   }
   public void setLink(Node l)
   {
	   link=l;
	   
   }
   public void setData(int d)
   {
	   data=d;
	   
   }
   public Node getLink(Node l)
   {
	   return link;
	   
   }
   public int getData(Node l)
   {
	   return data;
	   
   }
}
public class slinkedlist {
	
public static void main(String[] args)
{
	
}

}
